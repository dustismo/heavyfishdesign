{
    "params": {
        "offset": ".007",
        "width": 20,
        "height": 12,
        "box_width": 3,
        "box_height": 7
    },
    "hidden" : [
        {
            "type": "connector",
            "id" : "connector_1",
            "finger_width" : 0.25,
            "finger_height" : 0.2,
            "finger_space" : 0.4,
            "to" : {
                "x": "5",
                "y":0
            },
            "plug" : {
                "type" : "draw",
                "commands" : [
                    {
                        "command" : "move",
                        "to" : {"x": 0, "y": "finger_height"}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_space / 2","y":0}
                    },
                    
                    {
                        "command" : "rel_line",
                        "to" : {"x": 0,"y":"0-finger_height"}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_width","y":0}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": 0,"y":"finger_height"}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_space / 2","y":0}
                    },

                    {
                        "command" : "rel_move",
                        "to" : {"x": "0","y":"0-finger_height"},
                        "comment" : "Need to move back to the origin for the next iteration"
                    }
                ]
            },
            "socket": {
                "type" : "draw",
                "commands" : [
                    {
                        "command" : "move",
                        "to" : {"x": 0, "y": 0}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_space / 2","y":0}
                    },
                    
                    {
                        "command" : "rel_line",
                        "to" : {"x": 0,"y":"finger_height"}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_width","y":0}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": 0,"y":"0 - finger_height"}
                    },
                    {
                        "command" : "rel_line",
                        "to" : {"x": "finger_space / 2","y":0}
                    }
                ]
            }
        }
    ],
    "parts": [
        {
            "params" : {
                
            },
            "transforms": [
                {
                    "type": "join",
                    "close_path" : "true"
                },
                {
                    "type": "offset",
                    "distance" : "offset"
                }
            ],
            "components": [
                {
                    "type" : "draw",
                    "commands" : [
                        {
                            "command" : "move",
                            "to" : {"x": "0", "y": "0"}
                        },
                        {
                            "command" : "line",
                            "to" : {"x": "box_width", "y": 0}
                        }
                    ]
                },
                {
                    "type": "reference",
                    "component": "@connector_1",
                    "side": "plug",
                    "transforms": [
                        {
                            "type" : "mirror",
                            "axis" : "vertical"
                        },
                        {
                            "type" : "move",
                            "to" : {"x": 0, "y" : "box_height / 2"},
                            "handle" : "$MIDDLE_LEFT"
                        }
                    ],
                    "from": {
                        "x": 0,
                        "y": 0
                    },
                    "to" : {
                        "x": 0,
                        "y": "box_height"
                    }
                },
                {
                    "type": "reference",
                    "component": "@connector_1",
                    "side": "plug",
                    "transforms": [
                        {
                            "type" : "move",
                            "to" : {"x": "box_width", "y" : "box_height / 2"},
                            "handle" : "$MIDDLE_RIGHT"
                        }
                    ],
                    "from": {
                        "x": 0,
                        "y": 0
                    },
                    "to" : {
                        "x": 0,
                        "y": "box_height"
                    }
                },
                //This is the bottom side
                {
                    "type": "reference",
                    "component": "@connector_1",
                    "side": "plug",
                    "transforms": [
                        {
                            "type" : "mirror",
                            "axis" : "horizontal"
                        },
                        {
                            "type" : "move",
                            "to" : {"x": "box_width / 2", "y" : "box_height"},
                            "handle" : "$TOP_MIDDLE"
                        }
                    ],
                    "from": {
                        "x": 0,
                        "y": 0
                    },
                    "to" : {
                        "x": "box_width",
                        "y": 0
                    }
                }
            ]
        }
    ]
}
